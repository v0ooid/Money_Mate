<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/main_color"
    tools:context=".AppLock.AppLock6Activity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="303dp"
        android:layout_height="368dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout">

        <Button
            android:id="@+id/buttonLock61"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:backgroundTint="@color/main_color"
            android:fontFamily="@font/manrope_bold"
            android:text="1"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/buttonLock63"
            android:layout_width="65dp"
            android:layout_height="67dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:backgroundTint="@color/main_color"
            android:text="3"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/buttonLock60"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginTop="24dp"
            android:backgroundTint="@color/main_color"
            android:text="0"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/buttonLock68"
            app:layout_constraintStart_toStartOf="@+id/buttonLock68"
            app:layout_constraintTop_toBottomOf="@+id/buttonLock68" />

        <Button
            android:id="@+id/buttonLock62"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:backgroundTint="@color/main_color"
            android:text="2"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/buttonLock63"
            app:layout_constraintEnd_toStartOf="@+id/buttonLock63"
            app:layout_constraintStart_toEndOf="@+id/buttonLock61"
            app:layout_constraintTop_toTopOf="@+id/buttonLock61" />

        <Button
            android:id="@+id/buttonLock64"
            android:layout_width="65dp"
            android:layout_height="67dp"
            android:layout_marginTop="24dp"
            android:backgroundTint="@color/main_color"
            android:text="4"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/buttonLock61"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/buttonLock61"
            app:layout_constraintTop_toBottomOf="@+id/buttonLock61" />

        <Button
            android:id="@+id/buttonLock68"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginTop="24dp"
