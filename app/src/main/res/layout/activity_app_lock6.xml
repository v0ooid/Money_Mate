            android:text="8"
            android:textSize="24sp"
            app:layout_constraintEnd_toStartOf="@+id/buttonLock69"
            app:layout_constraintStart_toEndOf="@+id/buttonLock67"
            app:layout_constraintTop_toBottomOf="@+id/buttonLock65" />

        <Button
            android:id="@+id/buttonLock69"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginTop="24dp"
            android:backgroundTint="@color/main_color"
            android:text="9"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/buttonLock66"
            app:layout_constraintStart_toStartOf="@+id/buttonLock66"
            app:layout_constraintTop_toBottomOf="@+id/buttonLock66" />

        <Button
            android:id="@+id/buttonLock67"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginTop="24dp"
            android:backgroundTint="@color/main_color"
            android:text="7"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/buttonLock64"
            app:layout_constraintStart_toStartOf="@+id/buttonLock64"
            app:layout_constraintTop_toBottomOf="@+id/buttonLock64" />

        <Button
            android:id="@+id/buttonLock65"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginTop="24dp"
            android:backgroundTint="@color/main_color"
            android:text="5"
            android:textSize="24sp"
            app:layout_constraintEnd_toStartOf="@+id/buttonLock66"
            app:layout_constraintStart_toEndOf="@+id/buttonLock64"
            app:layout_constraintTop_toBottomOf="@+id/buttonLock62" />

        <Button
            android:id="@+id/buttonLock66"
            android:layout_width="65dp"
            android:layout_height="67dp"
            android:layout_marginTop="24dp"
            android:backgroundTint="@color/main_color"
            android:text="6"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/buttonLock63"
            app:layout_constraintStart_toStartOf="@+id/buttonLock63"
            app:layout_constraintTop_toBottomOf="@+id/buttonLock63" />

        <ImageView
            android:id="@+id/iV6Back"
            android:layout_width="65dp"
            android:layout_height="65dp"
            android:layout_marginTop="24dp"
