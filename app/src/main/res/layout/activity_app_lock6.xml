            app:layout_constraintEnd_toEndOf="@+id/buttonLock69"
            app:layout_constraintStart_toStartOf="@+id/buttonLock69"
            app:layout_constraintTop_toBottomOf="@+id/buttonLock69"
            app:srcCompat="@drawable/outline_arrow_circle_left_24" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="201dp"
        android:layout_height="37dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline6">

        <ImageView
            android:id="@+id/dot4Digit61"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginStart="16dp"
            android:background="@drawable/bd_view_grey_oval"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/dot4Digit62"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginStart="16dp"
            android:background="@drawable/bd_view_grey_oval"
            app:layout_constraintBottom_toBottomOf="@+id/dot4Digit61"
            app:layout_constraintStart_toEndOf="@+id/dot4Digit61"
            app:layout_constraintTop_toTopOf="@+id/dot4Digit61" />

        <ImageView
            android:id="@+id/dot4Digit63"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginStart="16dp"
            android:background="@drawable/bd_view_grey_oval"
            app:layout_constraintBottom_toBottomOf="@+id/dot4Digit62"
            app:layout_constraintStart_toEndOf="@+id/dot4Digit62"
            app:layout_constraintTop_toTopOf="@+id/dot4Digit62"
            app:layout_constraintVertical_bias="0.0" />

        <ImageView
            android:id="@+id/dot4Digit65"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/bd_view_grey_oval"
            app:layout_constraintBottom_toBottomOf="@+id/dot4Digit66"
            app:layout_constraintEnd_toStartOf="@+id/dot4Digit66"
            app:layout_constraintTop_toTopOf="@+id/dot4Digit66" />

        <ImageView
            android:id="@+id/dot4Digit64"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/bd_view_grey_oval"
            app:layout_constraintBottom_toBottomOf="@+id/dot4Digit65"
            app:layout_constraintEnd_toStartOf="@+id/dot4Digit65"
            app:layout_constraintTop_toTopOf="@+id/dot4Digit65" />

        <ImageView
            android:id="@+id/dot4Digit66"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginEnd="16dp"
            android:background="@drawable/bd_view_grey_oval"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/textView39"
        android:layout_width="159dp"
        android:layout_height="32dp"
        android:fontFamily="@font/manrope_bold"
        android:text="@string/app_name"
        android:textColor="@color/white"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.3" />

</androidx.constraintlayout.widget.ConstraintLayout>